        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="guest" namespace="/guest" extends="struts-default">
		<action name="RegisterForm" class="nl.plaatsmarkt.actions.users.Registerform">
			<result name="success">/jsp/visitor/registrerenComplete.jsp</result>
			<result name="input">/jsp/visitor/registreren.jsp</result>
		</action>
		
		<action name="LoginForm" class="nl.plaatsmarkt.actions.users">
			<result name="success">/jsp/visitor/registrerenComplete.jsp</result>
